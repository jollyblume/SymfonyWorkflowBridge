{
    "name": "jollyblume/symfony-workflow-bridge",
    "description": "symfony/workflow bridge for jollyblume/workflow components",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jolly Blume",
            "email": "jollyblume2@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "jollyblume/marking-store": "@dev",
        "jollyblume/workflow-common": "@dev",
        "moontoast/math": "^1.1",
        "ramsey/uuid": "@dev",
        "symfony/workflow": "^4.2"
    },
    "require-dev": {
        "psr/log": "~1.1",
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "JBJ\\Workflow\\SymfonyBridge\\": "lib"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JBJ\\Workflow\\Tests\\SymfonyBridge\\": "tests"
        }
    }
}
